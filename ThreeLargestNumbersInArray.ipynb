    "def threeLarestNumsInArr(array):\n",
    "    array.sort()\n",
    "    lenght = len(array)\n",
    "    if(lenght<=3):\n",
    "        return array\n",
    "    else:\n",
    "        return [array[-3],array[-2],array[-1]]"
   ]
